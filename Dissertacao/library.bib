journal = {Mechatronics},
keywords = {3-D finite element analysis,Axial flow blood pump,Hybrid magnetic bearing,Magnetic bearing test rig},
number = {7},
pages = {1163--1169},
pmid = {22065892},
publisher = {Elsevier Ltd},
title = {{Optimization of a hybrid magnetic bearing for a magnetically levitated blood pump via 3-D FEA}},
url = {http://dx.doi.org/10.1016/j.mechatronics.2011.07.010},
volume = {21},
